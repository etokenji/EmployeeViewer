@{
    ViewData["Title"] = "EmployeeInfo";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <div class="table-base">
        <table id="employeeTable" class="table table-striped">
            <thead>
                <tr class="active">
                    <th>社員番号</th>
                    <th>名前</th>
                    <th>姓</th>
                    <th>フルネーム</th>
                    <th>部署コード</th>
                    <th>郵便番号</th>
                    <th>住所</th>
                    <th>電話番号</th>
                    <th>生年月日</th>
                    <th>性別</th>
                    <th>備考</th>
                    <th>登録日</th>
                    <th>更新日</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>

